<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  <!--Mapper 파일 인식  -->

<mapper namespace="TableMapper"><!--nameSpace의 이르은 파일명과 동일하게  -->
	
	<insert id="InsertTableA" parameterType="com.choa.table.TableADTO">
		insert into tablea values (#{num}, #{title}, #{writer})
	</insert>
	
	<insert id="InsertTableB" parameterType="com.choa.table.TableBDTO">
		insert into tableb values (#{num}, #{writer}, #{point})
	</insert>
	
</mapper>
